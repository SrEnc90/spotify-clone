{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@lib/*": ["./src/lib/*"],
			"@public/*": ["./public/*"],
			"@layouts/*": ["./src/layouts/*"],
			"@store/*": ["./src/store/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
